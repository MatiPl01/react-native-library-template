{
  "name": "library-name",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/runtime": "^7.20.0",
    "@react-native/metro-config": "^0.72.11",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/eslint": "^8",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.48",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18",
    "eslint": "^8.56.0",
    "eslint-config-react-native-matipl01": "^2.1.0",
    "get-yarn-workspaces": "^1.0.2",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "lint-staged": "^15.2.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-native": "^0.72.6",
    "react-native-builder-bob": "^0.23.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.2",
    "react-native-svg": "^14.1.0",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^22.0.12",
    "syncpack": "^12.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "bash -c tsc --noEmit",
      "prettier --write --ignore-unknown"
    ],
    "package.json": [
      "yarn format:deps"
    ]
  },
  "main": "dist/commonjs/index",
  "module": "dist/module/index",
  "packageManager": "yarn@4.0.2",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "react-native": "src/index",
  "resolutions": {
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "scripts": {
    "build": "yarn clean && bob build",
    "clean": "./scripts/clean.sh",
    "example:app": "yarn workspace example-app",
    "example:bare": "yarn workspace example-bare",
    "example:expo": "yarn workspace example-expo",
    "format:check": "prettier --check . --ignore-unknown",
    "format:code": "prettier --write . --ignore-unknown",
    "format:deps": "syncpack format",
    "husky:install": "./scripts/husky.sh",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "yarn husky:install",
    "prepare": "yarn build",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.json --noEmit && yarn example:app typecheck"
  },
  "type": "module",
  "types": "dist/typescript/index.d.ts",
  "workspaces": [
    "example/*",
    "src"
  ]
}
