{
  "name": "library-name",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.81",
    "@react-native/metro-config": "0.74.81",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.3",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^13.0.0",
    "@testing-library/react-native": "^12.4.5",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.1.0",
    "eslint-config-react-native-matipl01": "^3.0.2",
    "get-yarn-workspaces": "^1.0.2",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "^50.0.4",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-native": "0.73.6",
    "react-native-builder-bob": "^0.23.2",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^23.0.8",
    "syncpack": "^12.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "bash -c tsc --noEmit",
      "prettier --write --ignore-unknown"
    ],
    "package.json": [
      "yarn format:deps"
    ]
  },
  "main": "dist/commonjs/index",
  "module": "dist/module/index",
  "packageManager": "yarn@4.1.1",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "react-native": "src/index",
  "scripts": {
    "build": "yarn clean && bob build",
    "clean": "./scripts/clean.sh",
    "example:app": "yarn workspace example-app",
    "example:bare": "yarn workspace example-bare",
    "example:expo": "yarn workspace example-expo",
    "format:check": "prettier --check . --ignore-unknown",
    "format:code": "prettier --write . --ignore-unknown",
    "format:deps": "syncpack format",
    "husky:install": "./scripts/husky.sh",
    "lint": "eslint . --config ./eslint.config.js",
    "lint:fix": "eslint --fix . --config ./eslint.config.js",
    "postinstall": "yarn husky:install",
    "prepare": "yarn build",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.json --noEmit && yarn example:app typecheck"
  },
  "type": "module",
  "types": "dist/typescript/index.d.ts",
  "workspaces": [
    "example/*",
    "src"
  ]
}
